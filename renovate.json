{
  "extends": [
    "config:base",
    ":prHourlyLimitNone",
    ":rebaseStalePrs",
    "group:allNonMajor"
  ],
  "packageRules": [
    {
      "updateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "pip_requirements": {
    "fileMatch": [
      "requirements\\.txt",
      "requirements-test\\.txt"
    ]
  }
}
